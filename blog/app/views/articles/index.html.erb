<h2>Listing Articles</h2>


<table>
	<thead>
		<tr>
			<td>Title</td>
			<td>Text</td>
		</tr>
	</thead>
<% @articles.each do |f| %>
	<tr>
		<td><%= f.title %></td>
		<td><%= f.text %></td>
		<td><%= link_to("Show", article_path(f)) %></td>
		<td><%= link_to("Edit", edit_article_path(f)) %></td>
	</tr>
<% end %>
</table>